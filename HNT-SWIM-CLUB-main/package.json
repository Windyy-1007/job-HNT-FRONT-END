{
  "name": "hnt-swim-club-e2e-tests",
  "version": "1.0.0",
  "description": "Selenium E2E test suite for HNT Swim Club e-commerce application",
  "scripts": {
    "test": "mocha tests/**/*.test.js --timeout 60000 --exit",
    "test:headless": "HEADLESS=true mocha tests/**/*.test.js --timeout 60000 --exit",
    "test:single": "mocha --timeout 60000 --exit",
    "test:auth": "mocha tests/auth.test.js --timeout 60000 --exit",
    "test:admin": "mocha tests/admin.test.js --timeout 60000 --exit",
    "test:shopping": "mocha tests/shopping.test.js --timeout 60000 --exit",
    "test:orders": "mocha tests/orders.test.js --timeout 60000 --exit",
    "test:report": "mocha tests/**/*.test.js --timeout 60000 --reporter mochawesome --reporter-options reportDir=test-reports,reportFilename=report --exit"
  },
  "keywords": [
    "selenium",
    "e2e",
    "testing",
    "automation",
    "mocha"
  ],
  "author": "QA Team",
  "license": "MIT",
  "devDependencies": {
    "selenium-webdriver": "^4.16.0",
    "mocha": "^10.2.0",
    "chai": "^4.3.10",
    "mochawesome": "^7.1.3",
    "dotenv": "^16.3.1"
  }
}
